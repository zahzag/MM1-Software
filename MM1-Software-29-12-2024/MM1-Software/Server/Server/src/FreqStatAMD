#!/bin/bash
 date  >> /home/rootie/ServerMultiThreads/bin/Freq_1.csv;
 echo "Time_in_Transition" >> /home/rootie/ServerMultiThreads/bin/Freq_1.csv;
 cat -n /sys/devices/system/cpu/cpu11/cpufreq/stats/time_in_state >> /home/rootie/ServerMultiThreads/bin/Freq_1.csv;
#echo "Total_Transition" >> /home/rootie/ServerMultiThreads/bin/Freq_1.csv;
#cat -n /sys/devices/system/cpu/cpu11/cpufreq/stats/total_trans >> /home/rootie/ServerMultiThreads/bin/Freq_1.csv;
 echo "Transition_Table" >> /home/rootie/ServerMultiThreads/bin/Freq_1.csv;
 cat -n /sys/devices/system/cpu/cpu11/cpufreq/stats/trans_table >> /home/rootie/ServerMultiThreads/bin/Freq_1.csv;




